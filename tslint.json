{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "jsx-boolean-value": [
      "never"
    ],
    "object-literal-sort-keys": false
  }
}
